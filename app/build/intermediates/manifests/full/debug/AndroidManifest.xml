<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cse110.clicker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.cse110.clicker.DashboardActivity" />
        <activity android:name="com.cse110.clicker.RegisterActivity" />
        <activity
            android:name="com.cse110.clicker.LoginActivity"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cse110.clicker.CreateSessionActivity" />
        <activity android:name="com.cse110.clicker.JoinSessionActivity" />
        <activity android:name="com.cse110.clicker.AnswerQuestionActivity" />
        <activity android:name="com.cse110.clicker.CreateQuestionActivity" />
    </application>

</manifest>